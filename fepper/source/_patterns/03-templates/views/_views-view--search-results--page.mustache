{{> 03-templates/views/__views-view-prefix.mustache }}

  {{!///////////////////////////////////////////////////////////////////////////
  /// FILTERS START //////////////////////////////////////////////////////////}}

  {{# views_view_exposed? }}
    <div class="view-filters">
      {{{ views_view_exposed }}}

      {{# search_results? }}
        {{> 01-molecules/listing-items/titles-listing-filters }}
      {{/ search_results? }}

    </div>
  {{/ views_view_exposed? }}

  {{! FILTERS END //////////////////////////////////////////////////////////////
  ////////////////////////////////////////////////////////////////////////////}}

  {{!///////////////////////////////////////////////////////////////////////////
  /// HEADERS START //////////////////////////////////////////////////////////}}

  {{# views_view_header? }}
    <div class="view-header">
      {{{ views_view_header }}}

      {{# search_results? }}
        {{> 01-molecules/listing-items/search-header }}
      {{/ search_results? }}

    </div>
  {{/ views_view_header? }}

  {{! HEADERS END //////////////////////////////////////////////////////////////
  ////////////////////////////////////////////////////////////////////////////}}


  {{# views_view_attachment_before? }}
    <div class="attachment attachment-before">
      {{{ views_view_attachment_before }}}
    </div>
  {{/ views_view_attachment_before? }}



  {{!///////////////////////////////////////////////////////////////////////////
  /// ROWS START /////////////////////////////////////////////////////////////}}

  {{# views_view_rows? }}
    <div class="view-content">

      {{# search_results? }}
        {{# search_results_rows }}
          {{> 01-molecules/listing-items/search-results }}
        {{/ search_results_rows }}
      {{/ search_results? }}

    </div>
  {{/ views_view_rows? }}

  {{! ROWS END /////////////////////////////////////////////////////////////////
  ////////////////////////////////////////////////////////////////////////////}}



  {{# views_view_empty? }}
    <div class="view-empty">
      {{{ views_view_empty }}}
    </div>
  {{/ views_view_empty? }}



  {{!///////////////////////////////////////////////////////////////////////////
  /// PAGERS START ///////////////////////////////////////////////////////////}}

  {{# views_view_pager? }}
    {{{ views_view_pager }}}

    {{# search_results? }}
      {{> 01-molecules/components/pager }}
    {{/ search_results? }}

  {{/ views_view_pager? }}

  {{! PAGERS END ///////////////////////////////////////////////////////////////
  ////////////////////////////////////////////////////////////////////////////}}


{{> 03-templates/views/__views-view-suffix.mustache }}
