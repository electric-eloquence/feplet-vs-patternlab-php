      {{> 03-templates/panes/panels-pane--section(poster_art\?: true) }}
    {{/ poster_art }}

    {{# title_available_and_buy_now }}
      {{> 03-templates/panes/panels-pane--section(available_and_buy_now\?: true) }}
    {{/ title_available_and_buy_now }}

    {{! has_title_page_video? }}
      {{# title_info}}
        {{# field_name_body}}
          {{> 03-templates/panes/panels-pane--section(title_synopsis\?: true) }}
        {{/ field_name_body}}
      {{/ title_info}}
    {{! has_title_page_video? }}

    {{! has_title_page_video? }}
      {{# title_page_video }}
        {{> 03-templates/panes/panels-pane--section(title_page_video\?: true) }}
      {{/ title_page_video }}
    {{! has_title_page_video? }}

    {{# 02_sections_stay_connected }}
      {{> 03-templates/panes/panels-pane--section(stay_connected\?: true) }}
    {{/ 02_sections_stay_connected }}
  {{/ title_top_iconic? }}


  {{# title_media_full_width? }}
    {{^ 04_title_detail_no_slider? }}
      {{# media_strip_titles_page }}
        {{> 03-templates/panes/panels-pane--section(media_strip\?: true) }}
      {{/ media_strip_titles_page }}
