    {{# 02_promos_promo_banner }}
      {{^ 04_title_detail_no_promo? }}
        {{> 03-templates/panes/panels-pane--section(promo_banner\?: true) }}
      {{/ 04_title_detail_no_promo? }}
    {{/ 02_promos_promo_banner }}
  {{/ title_media_full_width? }}


