  {{# title_bottom_full_width? }}
    {{# media_strip_downloads_and_games }}
      {{> 03-templates/panes/panels-pane--section(downloads_and_games\?: true) }}
    {{/ media_strip_downloads_and_games }}
  {{/ title_bottom_full_width? }}

