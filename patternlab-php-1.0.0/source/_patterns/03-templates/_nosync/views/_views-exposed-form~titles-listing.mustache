<form class="ctools-auto-submit-full-form ctools-auto-submit-processed jquery-once-3-processed" action="/" method="get" id="views-exposed-form-titles-landing-pages-panel-pane-1" accept-charset="UTF-8">
  <div>
    <div class="views-exposed-form">
      <div class="views-exposed-widgets clearfix">
        <div id="filters-toggle">Filters<span class="icon-arrow-down"></span></div>
        <div id="edit-custom-street-filter-wrapper" class="views-exposed-widget views-widget-filter-custom_street_filter">
          <label for="edit-custom-street-filter"><span class="hide-med hide-large">Release </span>Date<span class="icon-arrow-down"></span></label>
          <div class="views-widget">
            {{> molecules-custom-street-filter}}
          </div>
        </div>
        <div id="edit-field-format-value-wrapper" class="views-exposed-widget views-widget-filter-field_format_value">
          <label for="edit-field-format-value">Format<span class="icon-arrow-down"></span></label>
          <div class="views-widget">
            {{> molecules-field-format-value}}
          </div>
        </div>
        <div id="edit-field-genre-tid-wrapper" class="views-exposed-widget views-widget-filter-field_genre_tid">
          <label for="edit-field-genre-tid">Genre<span class="icon-arrow-down"></span></label>
          <div class="views-widget">
            {{> molecules-field-genre-tid}}
          </div>
        </div>
        {{#rating}}
        <div id="edit-field-rating-value-wrapper" class="views-exposed-widget views-widget-filter-field_rating_value">
          <label for="edit-field-rating-value">Rating<span class="icon-arrow-down"></span></label>
          <div class="views-widget">
            {{> molecules-field-rating-value}}
          </div>
        </div>
        {{/rating}}
        <div id="edit-sort-bef-combine-wrapper" class="views-exposed-widget views-widget-sort-sort_bef_combine">
          {{> molecules-sort-bef-combine}}
        </div>
        <div class="views-exposed-widget views-submit-button">
          {{> molecules-submit-button}}
        </div>
      </div>
    </div>
  </div>
</form>
